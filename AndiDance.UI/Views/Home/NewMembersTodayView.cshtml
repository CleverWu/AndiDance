                            <option>XXX门店</option>
                            <option>YYY门店</option>
                            <option>1</option>
                            <option>1</option>
                        </select>
                        <input placeholder="请输入名字或卡号" class="commonSelectInput-input">
                    </div>
                </div>
                <hr class="mt20">
                <div class="commonBoxContent">
                    <table class="todayCommonTable">
                        <tr>
                            <th>序号</th>
                            <th>姓名</th>
                            <th>卡号</th>
                            <th>联系电话</th>
                            <th>所属</th>
                            <th>余额</th>
                        </tr>
                        <tr>
