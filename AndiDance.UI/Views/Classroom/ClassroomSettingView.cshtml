<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>添加教室</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit">
    <link rel="stylesheet" type="text/css" href="./css/global.css">
    <link rel="stylesheet" type="text/css" href="./css/andi.css">
    <script src="./js/jquery/dist/jquery.min.js"></script>
    <!--[if lt IE 9]><script src="./js/html5shiv/dist/html5shiv.min.js"></script><![endif]-->
    <script src="./js/angular/angular.js"></script>
    <script src="./js/layer/layer.js"></script>
    <script src="./js/c_main.js"></script>
    <script src="./js/CommonService/CommonService.js"></script>

</head>
<body ng-app="AnDi">
    <div class="container" ng-controller="cMain">
        <header>
            <div class="top">
                <div class="top-logo" ng-click="goIndex()">
                    <p class="cn">安迪街舞会员管理系统</p>
                    <p class="en">ANDI MEMBER MANAGE SYSTEM</p>
                </div>
                <div class="info">
                    <a href="#" class="loginout"><img src="img/icon_loginout.png" alt=""></a>
                    <span class="info_txt">张飞云，欢迎登陆</span>
                </div>
            </div>
        </header>
        <nav class="navLeft">
            <ul>
                <li ng-repeat="nav in navList">
                    <div class="navTitle" ng-click="isActive($index)">{{nav.title}}</div>
                    <div class="navContent" ng-show="navIndex==$index">
                        <a ng-repeat="subNav in nav.content track by $index" href={{subNav.link}}><p>{{subNav.subName}}</p></a>
                    </div>
                </li>
            </ul>
        </nav>
        <div class="content" ng-controller="addClassRoom">
            <!--添加教室弹框-->
            <div class="commonPopUp addClassRoomTc">
                <div class="commonPopUp-title">
                    添加房间
                </div>
                <div class="commonPopUp-content">
                    <div class="mb10"><p>店名：安迪总部</p></div>
                    <div><p>房间名：<input type="text"></p></div>
                </div>
                <div class="commonPopUp-footer textCenter pb10">
                    <button class="btn_common inlineBlock t-ffffff b-11cd6e">确定</button><button class="btn_common inlineBlock ml5 ">取消</button>
                </div>
            </div>
            <!--编辑教室弹框-->
            <div class="commonPopUp editClassRoomTc">
                <div class="commonPopUp-title">
                    编辑教室信息
                </div>
                <div class="commonPopUp-content">
                    <div class="mb10"><p>店名：安迪总部</p></div>
                    <div><p>房间名：<input class="rectangleInput w240 h30" type="text" value="101"></p></div>
                </div>
                <div class="commonPopUp-footer textCenter pb10">
                    <button class="btn_common inlineBlock t-ffffff b-11cd6e">确定</button><button class="btn_common inlineBlock ml5 ">取消</button>
                </div>
            </div>
            <!--添加教室弹框-->
            <div class="commonPopUp classRoomUse">
                <div class="commonPopUp-title">
                    教室使用情况
                </div>
                <div class="commonPopUp-content">
                    <table>
                        <tr>
                            <th>周一</th>
                            <th>周二</th>
                            <th>周三</th>
                            <th>周四</th>
                            <th>周五</th>
                            <th>周六</th>
                            <th>周日</th>
                        </tr>
                        <tr>
                            <td><p>9:30-10:30 街舞基础</p><p>10:30-11:30 街舞进阶</p></td>
                            <td><p>9:30-10:30 街舞基础</p></td>
                            <td><p>9:30-10:30 街舞基础</p></td>
                            <td><p>9:30-10:30 街舞基础</p></td>
                            <td><p>9:30-10:30 街舞基础</p></td>
                            <td><p>9:30-10:30 街舞基础</p></td>
                            <td><p>9:30-10:30 街舞基础</p></td>
                        </tr>
                        <tr>
                            <td><p>13:00-15:30 街舞基础</p><p>15:30-18:00 街舞进阶</p></td>
                            <td><p>13:00-15:30 街舞基础</p><p>15:30-18:00 街舞进阶</p></td>
                            <td><p>13:00-15:30 街舞基础</p><p>15:30-18:00 街舞进阶</p></td>
                            <td><p>13:00-15:30 街舞基础</p><p>15:30-18:00 街舞进阶</p></td>
                            <td><p>13:00-15:30 街舞基础</p><p>15:30-18:00 街舞进阶</p></td>
                            <td><p>13:00-15:30 街舞基础</p><p>15:30-18:00 街舞进阶</p></td>
                            <td><p>13:00-15:30 街舞基础</p><p>15:30-18:00 街舞进阶</p></td>
                        </tr>
                    </table>
                </div>
                <div class="commonPopUp-footer textCenter pb10">
                    <button class="btn_common inlineBlock t-ffffff b-11cd6e">确定</button><button class="btn_common inlineBlock ml5 ">取消</button>
                </div>
            </div>
            <div class="commonBox">
                <div class="commonBoxTitle clearfix">
                    <p>添加教室</p>
                    <!--  <button>返回</button>
                      <button>提交</button>-->
                </div>
                <hr class="mt20">
                <div class="commonBoxContent pt5 pb5 pl5 pr5">
                    <div class="dancePlaceBox sub1">
                        <div class=" haveFzNext"></div>
                        <div class="dancePlaceBox-head clearfix">
                            <div class="dancePlaceBox-head-title fl">安迪总部</div><div class="dancePlaceBox-head-add fr" ng-click="addClassRoomTc()">+教室</div>
                        </div>
                        <div class="dancePlaceBox-content">
                            <div class="classroomList clearfix">
                                <div class="dancePlaceBox-content-place fl">102</div><div class="btn_addroomGroup fr"><button class="b-ff9900" ng-click="classRoomUse()">使用情况</button><button class="b-4e4193" ng-click="editClassRoomTc()">编辑</button><button class="b-ff0000">删除</button></div>
                            </div>
                            <div class="classroomList clearfix">
                                <div class="dancePlaceBox-content-place fl">102</div><div class="btn_addroomGroup fr"><button class="b-ff9900">使用情况</button><button class="b-4e4193">编辑</button><button class="b-ff0000">删除</button></div>
